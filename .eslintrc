{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "requireConfigFile": false
  },
  "plugins": ["prettier"],
  "extends": ["plugin:node/recommended", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-sync": 0,
    "no-process-exit": 0,
    "node/no-unpublished-import": 0
  }
}
